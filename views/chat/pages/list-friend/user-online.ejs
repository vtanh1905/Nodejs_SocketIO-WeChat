<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Friends</h3>
        <span data-toggle="tooltip" title="" class="badge bg-light-blue count"
            data-original-title="5 Users"><%= user.listFriend.length %></span>
        <!-- <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i
                    class="fa fa-minus"></i>
            </button>
        </div> -->
        <!-- /.box-tools -->
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <% user.listFriend.forEach(function(value, index) { %>
        <div class="user">
            <div class="col-md-8 user-avatar">
                <img class="contacts-list-img" src="images/users/<%= value.avatar %>" alt="User Image">
                <h5><%= value.username %></h5>
            </div>
            <div class="col-md-4"><button type="button" class="btn btn-block btn-primary btn-sm btn-w">Chat</button></div>
        </div>
        <% }); %>
    </div>
    <!-- /.box-body -->
</div>