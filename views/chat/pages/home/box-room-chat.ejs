<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Rooms</h3>
        <span data-toggle="tooltip" title="" class="badge bg-light-blue count"
            data-original-title="<%= listRoom.length %> Rooms"><%= listRoom.length %></span>

        <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i>
            </button>
        </div>
        <!-- /.box-tools -->
    </div>
    <!-- /.box-header -->
    <div class="box-body scroll-box">
        <% listRoom.forEach(function(value, index) { %>
            <div class="room">
                <div class="col-md-8 room-avatar">
                    <img class="rooms-list-img" src="images/rooms/<%= value.thumbnail %>" alt="User Image">
                    <h5 class="room-name"><%= value.name %></h5>
                    <small>Member: 5</small>
                </div>
                <div class="col-md-4">
                    <a href="room/<%= value._id %>" type="button" class="btn btn-block btn-primary btn-w btn-sm">Join</a>
                </div>
            </div>
        <% }); %>
    </div>
    <!-- /.box-body -->
</div>